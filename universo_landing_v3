    <script>
        lucide.createIcons();

        // Generador de Estrellas Realista
        const galaxy = document.getElementById('galaxy');
        const starCount = 400;
        for (let i = 0; i < starCount; i++) {
            const star = document.createElement('div');
            star.className = 'star';
            const size = Math.random() * 2.2;
            star.style.width = `${size}px`;
            star.style.height = `${size}px`;
            star.style.left = `${Math.random() * 100}%`;
            star.style.top = `${Math.random() * 100}%`;
            star.style.animationDelay = `${Math.random() * 8}s`;
            galaxy.appendChild(star);
        }

        function switchMission(missionId) {
            const n1 = document.getElementById('nebula-1');
            const n2 = document.getElementById('nebula-2');
            
            // Animación de color de nebulosa
            const colors = {
                cuasar: { c1: 'rgba(0, 255, 195, 0.1)', c2: 'rgba(23, 7, 116, 0.2)' },
                propulsion: { c1: 'rgba(255, 137, 40, 0.08)', c2: 'rgba(23, 7, 116, 0.2)' },
                sistema: { c1: 'rgba(109, 40, 217, 0.12)', c2: 'rgba(0, 255, 195, 0.04)' }
            };

            n1.style.backgroundColor = colors[missionId].c1;
            n2.style.backgroundColor = colors[missionId].c2;

            document.querySelectorAll('.mission-view').forEach(view => {
                view.classList.remove('active');
            });
            document.getElementById('mission-' + missionId).classList.add('active');

            document.querySelectorAll('.tab-btn').forEach(btn => {
                btn.classList.remove('active');
            });
            document.getElementById('tab-' + missionId).classList.add('active');

            window.scrollTo({ top: 0, behavior: 'smooth' });
            lucide.createIcons();
        }
    </script>
</body>
</html>
